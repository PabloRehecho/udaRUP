{
   "files": {
      "include": "scss/**/*.s+(a|c)ss",
      "ignore": [
         "scss/main.scss",
         "scss/rup-jqueryui-theme.scss",
         "scss/rup-theme.scss",
         "scss/rup-base.scss"
      ]
   },
   "rules": {
      "indentation": [
         1,
         {
            "size": 4
         }
      ],
      "nesting-depth": [
         1,
         {
            "max-depth": 3
         }
      ],
      "no-transition-all": 0,
      "class-name-format": 0,
      "variable-name-format": 0,
      "final-newline": 0
   }
}