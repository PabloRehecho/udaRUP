<div id="pagination_{{datatableId}}" class="col-sm-5 text-center text-sm-left">
	<i class="mdi mdi-view-list" aria-hidden="true" title="{{resultNumText}}"></i>
  	<span id="rup_jqtable_selectedElements_{{datatableId}}" class="rup-table-detail-currentElement" >
    	{{resultNumText}}
  	</span>
</div>
<div id="pag_{{datatableId}}" class="col-sm-7 text-center text-sm-right mt-3 mt-sm-0">
    <button type="button" id="first_{{datatableId}}" class="btn-material btn-material-sm btn-material-secondary-low-emphasis" alt="{{labelFirst}}">{{labelFirst}}</button>
    <button type="button" id="back_{{datatableId}}" class="btn-material btn-material-sm btn-material-secondary-low-emphasis" alt="{{labelPrev}}">{{labelPrev}}</button>
    <button type="button" id="forward_{{datatableId}}" class="btn-material btn-material-sm btn-material-secondary-low-emphasis" alt="{{labelNext}}">{{labelNext}}</button>
    <button type="button" id="last_{{datatableId}}" class="btn-material btn-material-sm btn-material-secondary-low-emphasis" alt="{{labelLast}}">{{labelLast}}</button>
</div>